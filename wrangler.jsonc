{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "llm-todo",
  "main": ".open-next/worker.js",
  "compatibility_date": "2025-01-01",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  "services": [
    {
      "binding": "WORKER_SELF_REFERENCE",
      "service": "llm-todo"
    }
  ],
  // D1 binding â€” database_id is injected at deploy time from GitHub Secrets
  // to keep this public repo safe. See SETUP.md for details.
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "llm-todo",
      "database_id": "placeholder",
      "migrations_dir": "drizzle/migrations"
    }
  ]
}

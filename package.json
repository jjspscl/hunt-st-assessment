{
  "name": "hunt-st-assessment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npx @cloudflare/next-on-pages",
    "preview": "wrangler pages dev .vercel/output/static",
    "deploy": "wrangler pages deploy .vercel/output/static",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply chat-task-tracker-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply chat-task-tracker-db --remote",
    "db:studio": "drizzle-kit studio",
    "reset": "node -e \"fetch('http://localhost:3000/api/admin/reset',{method:'POST'}).then(r=>r.json()).then(console.log).catch(console.error)\"",
    "test": "vitest run",
    "test:idem": "vitest run idempotency"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.37",
    "@hono/node-server": "^1.19.9",
    "@openrouter/ai-sdk-provider": "^2.2.3",
    "@tanstack/react-query": "^5.90.21",
    "ai": "^6.0.105",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.12.3",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "better-sqlite3": "^12.6.2",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18",
    "wrangler": "^4.69.0"
  }
}
